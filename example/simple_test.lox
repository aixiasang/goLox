// 基本算术运算
3 + 4 * 5;
2 - 8 / 4;
10 % 3;

// 组合运算
(2 + 3) * 4;
8 / (3 - 1);
-(5 + 2);

// 比较运算
5 > 3;
10 <= 10;
42 == 42;
7 != 8;

// 逻辑运算
true and false;
true or false;
!true;

// 字符串操作
"Hello" + " " + "World";

// 三元运算符
5 > 3 ? "大于" : "小于等于";

// 变量声明和赋值
var x = 10;
var y = 20;
x + y;

// 代码块作用域
{
  var a = 1;
  var b = 2;
  a + b;
}
